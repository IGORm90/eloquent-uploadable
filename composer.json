{
    "name": "sergeymiracle/eloquent-uploadable",
    "description": "A trait to automatically handle file uploads for Laravel models.",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*|5.4.*",
        "intervention/image": "^2.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "SergeyMiracle",
            "email": "sergeymiracle@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "SergeyMiracle\\Uploadable\\": "src/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "SergeyMiracle\\Uploadable\\UploadableServiceProvider"
        ]
      }
    },
    "suggest": {
        "spatie/image-optimizer": "Easily optimize images using PHP"
    },
    "config": {
        "sort-packages": true
    }
}
