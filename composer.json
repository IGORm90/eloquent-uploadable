{
    "name": "sergeymiracle/eloquent-uploadable",
    "description": "A trait to automatically handle file uploads for Laravel models.",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^8.0"

    },
    "license": "MIT",
    "authors": [
        {
            "name": "SergeyMiracle",
            "email": "sergeymiracle@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "SergeyMiracle\\Uploadable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SergeyMiracle\\Uploadable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --no-configuration SergeyMiracle\\Uploadable\\Tests\\UploadableTest tests/UploadableTest.php"
    },
    "extra": {
      "laravel": {
        "providers": [
          "SergeyMiracle\\Uploadable\\UploadableServiceProvider"
        ]
      }
    },
    "config": {
        "sort-packages": true
    }
}
